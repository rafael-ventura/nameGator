IAAA,WAnCA,uBAmCA,KAnCA,EAmCA;AACA,WAAA,OAAA,CAAA,MAAA,CAAA,KAAA,QAAA,CAAA,OAAA,CAAA,KAAA,CAAA,EAAA,CAAA;AACA,KArCA;AAsCA,IAAA,WAtCA,yBAsCA;AAAA;;AACA,MAAA,KAAA,CAAA;AACA,QAAA,GAAA,EAAA,uBADA;AAEA,QAAA,MAAA,EAAA,MAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,KAAA;AADA;AAHA,OAAA,CAAA,CAcA,IAdA,CAcA,UAAA,QAAA,EAAA;AACA,YAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,KAAA,CAAA,IAAA,CAAA,QAAA;AACA,OAjBA;AAkBA,KAzDA;AA0DA,IAAA,UA1DA,wBA0DA;AAAA;;AACA,MAAA,KAAA,CAAA;AACA,QAAA,GAAA,EAAA,uBADA;AAEA,QAAA,MAAA,EAAA,MAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,KAAA;AADA;AAHA,OAAA,CAAA,CAcA,IAdA,CAcA,UAAA,QAAA,EAAA;AACA,YAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA,CAAA,IAAA,CAAA,OAAA;AACA,OAjBA;AAkBA;AA7EA,GAXA;AA0FA,EAAA,QAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,uBAAA;AACA,UAAA,OAAA,GAAA,EAAA;;AAFA,iDAGA,KAAA,QAHA;AAAA;;AAAA;AAGA,4DAAA;AAAA,cAAA,MAAA;;AAAA,sDACA,KAAA,OADA;AAAA;;AAAA;AACA,mEAAA;AAAA,kBAAA,KAAA;AACA,kBAAA,IAAA,GAAA,MAAA,CAAA,WAAA,GAAA,KAAA,CAAA,WAAA;AACA,kBAAA,GAAA,GAAA,IAAA,CAAA,WAAA,EAAA;AACA,kBAAA,QAAA,0DAAA,GAAA,cAAA;AACA,cAAA,OAAA,CAAA,IAAA,CAAA;AACA,gBAAA,IAAA,EAAA,IADA;AAEA,gBAAA,QAAA,EAAA;AAFA,eAAA;AAIA;AATA;AAAA;AAAA;AAAA;AAAA;AAUA;AAbA;AAAA;AAAA;AAAA;AAAA;;AAcA,aAAA,OAAA;AACA;AAhBA,GA1FA;AA4GA,EAAA,OA5GA,qBA4GA;AACA,SAAA,WAAA;AACA,SAAA,UAAA;AACA;AA/GA,CAAA","sourcesContent":["<template>\n  <div>\n    <div id=\"main\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-md\">\n            <AppItemList\n              title=\"Prefixo\"\n              v-bind:items=\"prefixes\"\n              v-on:addItem=\"addPrefix\"\n              v-on:deleteItem=\"deletePrefix\"\n            ></AppItemList>\n          </div>\n          <div class=\"col-md\">\n            <AppItemList\n              title=\"Sufixo\"\n              v-bind:items=\"sufixes\"\n              v-on:addItem=\"addSufix\"\n              v-on:deleteItem=\"deleteSufix\"\n            ></AppItemList>\n          </div>\n        </div>\n        <br />\n        <h5>\n          Dominios <span class=\"badge badge-info\"> {{ domains.length }}</span>\n        </h5>\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <ul class=\"list-group\">\n              <li\n                class=\"list-group-item\"\n                v-for=\"domain in domains\"\n                v-bind:key=\"domain.name\"\n              >\n                <div class=\"row\">\n                  <div class=\"col-md\">\n                    {{ domain.name }}\n                  </div>\n                  <div class=\"col-md text-right\">\n                    <a\n                      class=\"btn btn-info\"\n                      v-bind:href=\"domain.checkout\"\n                      target=\"_blank\"\n                    >\n                      <span class=\"fa fa-shopping-cart\"> </span>\n                    </a>\n                  </div>\n                </div>\n              </li>\n            </ul>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios/dist/axios';\nimport AppItemList from './AppItemList.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    AppItemList,\n  },\n  data: function() {\n    return {\n      prefixes: [],\n      sufixes: [],\n    };\n  },\n  methods: {\n    addPrefix(prefix) {\n      axios({\n        url: 'http://localhost:4000',\n        method: 'post',\n        data: {\n          query: `\n            mutation ($item: ItemInput) {\n              newPrefix: saveItem(item: $item) {\n                id\n                type\n                description\n              }\n            }\n          `,\n          variables: {\n            item: {\n              type: 'prefix',\n              description: prefix,\n            },\n          },\n        },\n      })\n      .then(response =>{\n        const query = response.data;\n        const newPrefix = query.data.newPrefix;\n        this.prefixes.push(newPrefix.description);\n      });\n    },\n    addSufix(sufix) {\n      this.sufixes.push(sufix);\n    },\n    deletePrefix(prefix) {\n      this.prefixes.splice(this.prefixes.indexOf(prefix), 1);\n    },\n    deleteSufix(sufix) {\n      this.sufixes.splice(this.prefixes.indexOf(sufix), 1);\n    },\n    getPrefixes() {\n      axios({\n        url: 'http://localhost:4000',\n        method: 'post',\n        data: {\n          query: `\n          {\n            prefixes: items (type: \"prefix\") {\n              id\n              type\n              description\n            }\n          }\n        `,\n        },\n      }).then((response) => {\n        const query = response.data;\n        this.prefixes = query.data.prefixes;\n      });\n    },\n    getSufixes() {\n      axios({\n        url: 'http://localhost:4000',\n        method: 'post',\n        data: {\n          query: `\n          {\n            sufixes: items (type: \"sufix\") {\n              id\n              type\n              description\n            }\n          }\n        `,\n        },\n      }).then((response) => {\n        const query = response.data;\n        this.sufixes = query.data.sufixes;\n      });\n    },\n  },\n  computed: {\n    domains() {\n      console.log('generating domains...');\n      const domains = [];\n      for (const prefix of this.prefixes) {\n        for (const sufix of this.sufixes) {\n          const name = prefix.description + sufix.description;\n          const url = name.toLowerCase();\n          const checkout = `https://checkout.hostgator.com.br/?a=add&sld=${url}&tld=.com`;\n          domains.push({\n            name,\n            checkout,\n          });\n        }\n      }\n      return domains;\n    },\n  },\n  created() {\n    this.getPrefixes();\n    this.getSufixes();\n  },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":"src/components"}]}