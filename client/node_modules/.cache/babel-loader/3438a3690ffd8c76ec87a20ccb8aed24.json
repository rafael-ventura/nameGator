A,EAAA;AACA,UAAA,KAAA,oHADA;AAMA,UAAA,SAAA,EAAA;AACA,YAAA,EAAA,EAAA,MAAA,CAAA;AADA;AANA;AAHA,OAAA,CAAA,CAaA,IAbA,CAaA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,QAAA;AACA,OAfA;AAgBA,KA9CA;AA+CA,IAAA,WA/CA,uBA+CA,KA/CA,EA+CA;AACA,WAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,KAAA,KAAA,CAAA,KAAA,CAAA,OAAA,CAAA,KAAA,CAAA,EAAA,CAAA;AACA,KAjDA;AAkDA,IAAA,QAlDA,oBAkDA,IAlDA,EAkDA;AAAA;;AACA,MAAA,KAAA,CAAA;AACA,QAAA,GAAA,EAAA,uBADA;AAEA,QAAA,MAAA,EAAA,MAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,KAAA,uMADA;AAUA,UAAA,SAAA,EAAA;AACA,YAAA,IAAA,EAAA;AADA;AAVA;AAHA,OAAA,CAAA,CAiBA,IAjBA,CAiBA,UAAA,QAAA,EAAA;AACA,YAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,IAAA,IAAA,KAAA,CAAA,IAAA,CAAA,KAAA;AACA,OApBA;AAqBA;AAxEA,GAbA;AAuFA,EAAA,QAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,uBAAA;AACA,UAAA,OAAA,GAAA,EAAA;;AAFA,iDAGA,KAAA,KAAA,CAAA,MAHA;AAAA;;AAAA;AAGA,4DAAA;AAAA,cAAA,MAAA;;AAAA,sDACA,KAAA,KAAA,CAAA,KADA;AAAA;;AAAA;AACA,mEAAA;AAAA,kBAAA,KAAA;AACA,kBAAA,IAAA,GAAA,MAAA,CAAA,WAAA,GAAA,KAAA,CAAA,WAAA;AACA,kBAAA,GAAA,GAAA,IAAA,CAAA,WAAA,EAAA;AACA,kBAAA,QAAA,0DAAA,GAAA,cAAA;AACA,cAAA,OAAA,CAAA,IAAA,CAAA;AACA,gBAAA,IAAA,EAAA,IADA;AAEA,gBAAA,QAAA,EAAA;AAFA,eAAA;AAIA;AATA;AAAA;AAAA;AAAA;AAAA;AAUA;AAbA;AAAA;AAAA;AAAA;AAAA;;AAcA,aAAA,OAAA;AACA;AAhBA,GAvFA;AAyGA,EAAA,OAzGA,qBAyGA;AACA,SAAA,QAAA,CAAA,QAAA;AACA,SAAA,QAAA,CAAA,OAAA;AACA;AA5GA,CAAA","sourcesContent":["<template>\n  <div>\n    <div id=\"main\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-md\">\n            <AppItemList\n              title=\"Prefixo\"\n              type=\"prefix\"\n              v-bind:items=\"items.prefix\"\n              v-on:addItem=\"addItem\"\n              v-on:deleteItem=\"deleteItem\"\n            ></AppItemList>\n          </div>\n          <div class=\"col-md\">\n            <AppItemList\n              title=\"Sufixo\"\n              type=\"sufix\"\n              v-bind:items=\"items.sufix\"\n              v-on:addItem=\"addItem\"\n              v-on:deleteItem=\"deleteItem\"\n            ></AppItemList>\n          </div>\n        </div>\n        <br />\n        <h5>\n          Dominios <span class=\"badge badge-info\"> {{ domains.length }}</span>\n        </h5>\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <ul class=\"list-group\">\n              <li\n                class=\"list-group-item\"\n                v-for=\"domain in domains\"\n                v-bind:key=\"domain.name\"\n              >\n                <div class=\"row\">\n                  <div class=\"col-md\">\n                    {{ domain.name }}\n                  </div>\n                  <div class=\"col-md text-right\">\n                    <a\n                      class=\"btn btn-info\"\n                      v-bind:href=\"domain.checkout\"\n                      target=\"_blank\"\n                    >\n                      <span class=\"fa fa-shopping-cart\"> </span>\n                    </a>\n                  </div>\n                </div>\n              </li>\n            </ul>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios/dist/axios';\nimport AppItemList from './AppItemList.vue';\n\nexport default {\n  name: 'App',\n  components: {\n    AppItemList,\n  },\n  data: function() {\n    return {\n      items: {\n        prefix: [],\n        sufix: [],\n      },\n    };\n  },\n  methods: {\n    addItem(item) {\n      axios({\n        url: 'http://localhost:4000',\n        method: 'post',\n        data: {\n          query: `\n            mutation ($item: ItemInput) {\n              newItem: saveItem(item: $item) {\n                id\n                type\n                description\n              }\n            }\n          `,\n          variables: {\n            item\n          },\n        },\n      }).then((response) => {\n        const query = response.data;\n        const newItem = query.data.newItem;\n        this.items[item.type].push(newItem);\n      });\n    },\n    addSufix(sufixs) {\n      this.items.sufix.push(sufixs);\n    },\n    deletePrefix(prefix) {\n      //this.prefixes.splice(this.prefixes.indexOf(prefix), 1);\n      axios({\n        url: 'http://localhost:4000',\n        method: 'post',\n        data: {\n          query: `\n            mutation ($id: Int) {\n              deletedPrefix: deleteItem(id: $id)\n            }\n          `,\n          variables: {\n            id: prefix.id,\n          },\n        },\n      }).then(() => {\n        this.getItems('prefix');\n      });\n    },\n    deleteSufix(sufix) {\n      this.items.sufix.splice(this.items.sufix.indexOf(sufix), 1);\n    },\n    getItems(type) {\n      axios({\n        url: 'http://localhost:4000',\n        method: 'post',\n        data: {\n          query: `\n            query ($type: String) {\n              items : items (type: $type) {\n                id\n                type\n                description\n              }\n            }\n        `,\n          variables: {\n            type,\n          },\n        },\n      }).then((response) => {\n        const query = response.data;\n        this.items[type] = query.data.items;\n      });\n    },\n  },\n  computed: {\n    domains() {\n      console.log('generating domains...');\n      const domains = [];\n      for (const prefix of this.items.prefix) {\n        for (const sufix of this.items.sufix) {\n          const name = prefix.description + sufix.description;\n          const url = name.toLowerCase();\n          const checkout = `https://checkout.hostgator.com.br/?a=add&sld=${url}&tld=.com`;\n          domains.push({\n            name,\n            checkout,\n          });\n        }\n      }\n      return domains;\n    },\n  },\n  created() {\n    this.getItems('prefix');\n    this.getItems('sufix');\n  },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":"src/components"}]}